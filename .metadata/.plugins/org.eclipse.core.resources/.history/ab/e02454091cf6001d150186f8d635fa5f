function numTest(num) {
	console.log(num);
	console.log(typeof num); // text 상자 안에 있는 건 무조건 문자열을 반환한다. string
	console.log(123 + num); // 숫자 + 문자열 => 문자열 연결 
	console.log('123' + num); // 문자열 + 문자열 => 문자열 연결 
	console.log(num + 123); // 문자열 + 숫자 => 문자열 연결 
	
//	덧셈을 제외한 나머지 연산은 문자열과 숫자를 연산해도 결과가 숫자로 처리된다.	
	console.log(num - 123);
	console.log(num * 123);
	console.log(num / 123);
	console.log(num % 123);
//	console.log(num ** 123);
}