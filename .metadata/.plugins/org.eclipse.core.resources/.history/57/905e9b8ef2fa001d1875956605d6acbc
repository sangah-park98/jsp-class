$(() => {
	$('.error').hide();
	
	$('#single').submit(function () {
		let userID = $('#infoBox').val().trim();
		if(userID == null || userID == '') {
			$('#infoBox').val('');
			$('#infoBox').focus();
			$('.error').show();
			return false;
		}
		return true;
	});
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
});