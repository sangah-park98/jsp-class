function evalTest() {
	let str = document.getElementsByName('evalValue')[0];
	
	console.log(str);
	console.log(str.value);
	console.log(typeof str.value);
	
	if(confirm('연산식을 올바르게 입력했나요?\n입력한 연산식: ' + str.value)) {
		
	} else {
		str.value = '';
		str.focus();
	}
	
	console.log(eval(str.value));
	
}