function choice2() {
	let text = $('input:radio').eq(0).val();
	$('#a').eq(0).html('<h1>' + text + '</h1>');
}

function choice3() {
	let text = $('input:checkbox').eq(0).val();
	$('#a').eq(0).html('<h1>' + text + '</h1>')
					
}